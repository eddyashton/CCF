      "transactions": [
        {
          "method": "LOG_record",
          "params": {
            "id": 42,
            "msg": "Hello world"
          },
          "expected_result": true
        },
        {
          "verb": "GET",
          "method": "LOG_get",
          "params": {
            "id": 42
          },
          "expected_result": {
            "msg": "Hello world"
          }
        }
      ]
    }
  ]
